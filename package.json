{
    "name": "leav_core",
    "version": "1.0.0",
    "description": "",
    "main": "build/index.js",
    "scripts": {
        "start:watch": "onchange -i 'src/**/*.ts'  -- npm run start:dev",
        "start:files": "onchange -i 'src/**/*.ts'  -- node --inspect=0.0.0.0:9230 -r ts-node/register src/ --filesManager",
        "start:dev": "node --inspect=0.0.0.0:9229 -r ts-node/register src/ --server",
        "start:build": "npm run build && node . --server",
        "test:watch": "jest -c jest-unit-config.json --watch",
        "test": "jest -c jest-unit-config.json",
        "test:e2e": "npm run test:e2e:api",
        "test:e2e:api": "NODE_ENV=test jest -c jest-e2e-api-config.json",
        "test:e2e:filesManager": "NODE_ENV=test jest -c jest-e2e-filesManager-config.json",
        "lint": "tslint --fix -p .",
        "lint:test": "tslint -p .",
        "lint:watch": "watch 'npm run lint' src/",
        "prettier": "prettier --write 'src/**/*.ts'",
        "dev": "run-p lint:watch start:watch test:watch",
        "precommit": "lint-staged && npm test",
        "db:migrate": "ts-node src/ --migrate",
        "import": "ts-node src/ import --clear",
        "benchmark": "ts-node src/ benchmark",
        "build": "scripts/build.js",
        "tscheck": "tsc -p .",
        "tscheck:watch": "tsc -w -p ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://Jerome_MILLOT@bitbucket.org/leav_ca/leav_core.git"
    },
    "author": "",
    "license": "ISC",
    "homepage": "https://bitbucket.org/leav_ca/leav_core#readme",
    "devDependencies": {
        "@types/debug": "0.0.30",
        "@types/glob": "^5.0.35",
        "@types/hapi": "^18.0.2",
        "@types/jest": "^23.0.0",
        "@types/lodash": "^4.14.109",
        "@types/node": "^12.12.6",
        "@types/uuid": "^7.0.2",
        "@types/winston": "^2.3.9",
        "@types/hapi__joi": "^16.0.12",
        "apollo-server-testing": "^2.7.0",
        "axios": "^0.19.0",
        "glob": "^7.1.6",
        "husky": "^0.14.3",
        "jest": "^24.1.0",
        "lint-staged": "^7.1.3",
        "moment-timezone": "^0.5.25",
        "nodemon": "^1.17.5",
        "npm-run-all": "^4.1.3",
        "onchange": "^4.0.0",
        "prettier": "^1.19.1",
        "ts-jest": "^24.0.0",
        "ts-node": "^6.2.0",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.3.1",
        "typescript": "^3.7.2",
        "watch": "^1.0.2"
    },
    "dependencies": {
        "@hapi/boom": "^7.4.2",
        "@hapi/hapi": "^18.4.1",
        "@hapi/joi": "^17.1.1",
        "@types/amqplib": "^0.5.13",
        "amqplib": "^0.5.5",
        "apollo-server": "^2.7.0",
        "apollo-server-core": "^2.7.0",
        "apollo-server-hapi": "^2.7.0",
        "app-root-path": "^3.0.0",
        "arangojs": "^6.11.1",
        "awilix": "^3.0.8",
        "bcryptjs": "^2.4.3",
        "commander": "^2.20.0",
        "deep-extend": "^0.5.1",
        "graphql": "^14.4.2",
        "graphql-errors": "^2.1.0",
        "graphql-extensions": "0.0.10",
        "graphql-tools": "^3.0.2",
        "graphql-type-json": "^0.3.1",
        "hapi-auth-jwt2": "8.6.0",
        "i18next": "^19.0.3",
        "i18next-node-fs-backend": "^2.1.3",
        "jsonwebtoken": "^8.2.2",
        "lodash": "^4.17.11",
        "moment": "^2.22.2",
        "uuid": "^7.0.3",
        "winston": "^2.4.2",
        "winston-transport": "^4.1.0"
    },
    "lint-staged": {
        "*.ts": [
            "prettier --write",
            "tslint --fix",
            "git add"
        ]
    }
}
