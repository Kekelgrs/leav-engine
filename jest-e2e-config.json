{
    "moduleFileExtensions": [
        "ts",
        "js"
    ],
    "transform": {
        "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "src\/__tests__\/e2e\/.*(.test.ts)$",
    "globalSetup":
        "./src/__tests__/e2e/globalSetupHook.js",
    "globalTeardown":
        "./src/__tests__/e2e/teardown.js",
    "setupFiles": [
        "<rootDir>/src/__tests__/jestUtils.ts"
    ]
}