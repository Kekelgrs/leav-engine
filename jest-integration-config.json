{
    "moduleFileExtensions": [
        "ts",
        "js"
    ],
    "transform": {
        "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "src\/__tests__\/integration\/.*(.test.ts)$",
    "globalSetup": "./src/__tests__/integration/globalSetupHook.js",
    "globalTeardown": "./src/__tests__/integration/teardown.js",
    "setupFiles": [
        "<rootDir>/src/__tests__/jestUtils.ts"
    ]
}