{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./",
    /* /!\ Paths are relative to the src folder of each app. Each app must set its baseUrl to src */
    "paths": {
      "@leav-engine/*": ["../../../libs/*/src"]
    }
  },
  "exclude": [
      "node_modules"
  ]
}
